{
  "name": "puppeteer-es6modules-typescript-tsnode-jasmine",
  "version": "1.0.0",
  "description": "Puppeteer end-to-end test automation boilerplate project. Using ES6 modules, TypeScript and jasmine.",
  "scripts": {
    "pretty": "prettier --no-config './spec/*.ts' --write",
    "lint": "tslint -c tslint.json './spec/*.ts'",
    "test": "npx jasmine"
  },
  "keywords": [
    "puppeteer",
    "es6 modules",
    "typescript",
    "tsnode",
    "jasmine",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "@types/is-ci": "2.0.0",
    "@types/jasmine": "3.5.2",
    "@types/node": "12.12.26",
    "@types/puppeteer": "2.0.0",
    "husky": "3.1.0",
    "jasmine": "3.5.0",
    "lint-staged": "10.0.5",
    "prettier": "1.19.1",
    "puppeteer": "2.1.0",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5"
  },
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
